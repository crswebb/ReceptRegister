@page
@model ReceptRegister.Frontend.Pages.Auth.LogoutModel
@{
    ViewData["Title"] = "Logout";
}
<form method="post">
    <p>Are you sure you want to logout?</p>
    <button type="submit" class="btn btn--danger">Logout</button>
    <a asp-page="/Recipes/Index" class="btn btn--subtle btn--sm">Cancel</a>
</form>
